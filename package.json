{
  "name": "kalemat",
  "version": "0.3.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "knex": "^0.18.1",
    "mongoose": "^5.6.5",
    "mysql": "^2.17.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1",
    "raw-loader": "^3.0.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "scripts": {
    "build": "webpack ",
    "start": "node index.js",
    "test": "nodemon index.js",
    "db_up": "knex migrate:up --env production --knexfile server/db/knexfile.js",
    "db_down": "knex migrate:down --env production --knexfile server/db/knexfile.js"
  },
  "nodemonConfig": {
    "watch": [
      "server/server.js",
      "index.js",
      "server/db/mysql/mysql.js",
      "server/db/mongo/*"
    ],
    "delay": "2500"
  }
}
